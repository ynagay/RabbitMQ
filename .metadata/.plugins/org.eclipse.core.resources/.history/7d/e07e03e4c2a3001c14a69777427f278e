package ru.jb.rabbitmq.entities;

public class Person {
	
	private Long id;
	private String name;
	
	
	public Person(Long id, String name) {
		super();
		this.id = id;
		this.name = name;
	}
	
	public long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	
}
